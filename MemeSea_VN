OCDashBoard.Parent = MemeGui
OCDashBoard.BackgroundColor3 = Color3.new(0, 0, 0)
OCDashBoard.BackgroundTransparency = 0.5
OCDashBoard.Size = UDim2.new(0.05, 0, 0.1, 0)
OCDashBoard.TextScaled = true
OCDashBoard.Position = UDim2.new(0.88, 0, -0.1, 0)
OCDashBoard.TextColor3 = Color3.new(1, 1, 1)
OCDashBoard.Font = Enum.Font.PatrickHand
OCDashBoard.Text = "‚óáS·ªë Kh√¥ng"

local OCDashBoardCorner = Instance.new("UICorner")
OCDashBoardCorner.Parent = OCDashBoard
OCDashBoardCorner.CornerRadius = UDim.new(0, 10)

-- ScrollingFrame --

MemeTools.Parent = MemeGui
MemeTools.Size = UDim2.new(0.3, 0, 0.1, 0)
MemeTools.Position = UDim2.new(1.2, 0, 0.18, 0)
MemeTools.BackgroundColor3 = Color3.new(0, 0, 0)
MemeTools.BackgroundTransparency = 0.5
MemeTools.CanvasSize = UDim2.new(2, 0, 0, 0)
MemeTools.ScrollBarThickness = 0.1

local goalPosition1 = UDim2.new(0.695, 0, 0.18, 0)
local goalPosition2 = UDim2.new(1.2, 0, 0.18, 0)

local tween1 = TweenService:Create(MemeTools, tweenInfo, {Position = goalPosition1})
local tween2 = TweenService:Create(MemeTools, tweenInfo, {Position = goalPosition2})

local isToggled = false
OCDashBoard.MouseButton1Click:Connect(function()
    if isToggled then
        tween2:Play()
        OCDashBoard.TextColor3 = Color3.new(1, 1, 1)
    else
        tween1:Play()
        OCDashBoard.TextColor3 = Color3.new(1, 0, 0)
    end
    isToggled = not isToggled
end)

-- More down below --

RandomSelect.Parent = MemeGui
RandomSelect.Size = UDim2.new(0.1, 0, 0.2, 0)  -- Adjusted to a more visible size
RandomSelect.Position = UDim2.new(-1, 0, 0.2, 0)  -- Positioned in the center of the screen
RandomSelect.BackgroundColor3 = Color3.new(0, 0, 0)
RandomSelect.BackgroundTransparency = 0.5
RandomSelect.CanvasSize = UDim2.new(0, 0, 2, 0)  -- Wider canvas size to enable scrolling
RandomSelect.ScrollBarThickness = 0.1

local RandomSelectCorner = Instance.new("UICorner")
