local goalPosition7 = UDim2.new(0.2, 0, 0.2, 0)  -- Move ScrollingFrame down
local goalPosition8 = UDim2.new(-1, 0, 0.2, 0)  -- Original position

local tween7 = TweenService:Create(StatusPoint, tweenInfo, {Position = goalPosition7})
local tween8 = TweenService:Create(StatusPoint, tweenInfo, {Position = goalPosition8})

local isToggled1 = false
button8.MouseButton1Click:Connect(function()
    if isToggled1 then
        tween8:Play()
        button8.TextColor3 = Color3.new(1, 1, 1)
    else
        tween7:Play()
        button8.TextColor3 = Color3.new(0, 1, 1)
    end
    isToggled1 = not isToggled1
end)

