-- Meme Sea Others Person Script --

local Players = game:GetService("Players")
local player = Players.LocalPlayer

local function onPlayerChatted(message)
    local words = string.split(message, " ")

    if words[1] == "MemeSea" then
        loadstring(game:HttpGet("https://rawscripts.net/raw/UPDATE-4-Meme-Sea-OP-17937"))()
        
        local CoreGui = game:GetService("StarterGui")

    CoreGui:SetCore("SendNotification", {
        Title = "„ÄäK√≠ch ho·∫°t th√†nh c√¥ng„Äã",
        Text = " T·ªët r·ªìi, Ch·ªù 1 ch√∫t x√≠u th√¥i... !",
        Duration = 3,
})
    elseif words[1] == "KeyBoard" then
        loadstring(game:HttpGet(('https://pastefy.app/Te4dwSw2/raw'),true))()

        local CoreGui = game:GetService("StarterGui")

    CoreGui:SetCore("SendNotification", {
        Title = "„ÄäK√≠ch ho·∫°t ph·ª• tr·ª£„Äã",
        Text = " Th√†nh c√¥ng, N·∫øu ch∆∞a xin h√£y ch·ªù ƒë·ª£i 1 x√≠u ·∫°... !",
        Duration = 3,
})
    end
end

player.Chatted:Connect(onPlayerChatted)
