TextBox.Parent = ScreenGui  -- Make sure ScreenGui is defined in your script
TextBox.Size = UDim2.new(0.3, 0, 0.1, 0)
TextBox.Position = UDim2.new(1.2, 0, 0.15, 0)
TextBox.BackgroundColor3 = Color3.new(0, 0, 0)
