Checker_All.Font = Enum.Font.PatrickHand
Checker_All.Text = "Notify[DeathCounters]"
Checker_All.Visible = true

local checking = false
local foundPlayers = {}

local function getNearestPlayer()
    local nearestPlayer = nil
    local shortestDistance = math.huge
    
    for _, otherPlayer in pairs(game.Players:GetPlayers()) do
        if otherPlayer ~= player then
            local distance = (player.Character.HumanoidRootPart.Position - otherPlayer.Character.HumanoidRootPart.Position).magnitude
            if distance < shortestDistance then
                shortestDistance = distance
                nearestPlayer = otherPlayer
            end
        end
    end
    
    return nearestPlayer
end

local function sendChatMessage(message)
    local args = {
        [1] = message,
        [2] = "All"
    }
    game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
end

local function checkForTool()
    while checking do
        wait(0.001)
        local nearestPlayer = getNearestPlayer()
        
        if nearestPlayer then
            local hasTool = nearestPlayer.Backpack:FindFirstChild("Death Counter")
            
            if hasTool and not foundPlayers[nearestPlayer.UserId] then
                sendChatMessage(nearestPlayer.Name .. " has activated Serious Mode.")
                wait(1)
                sendChatMessage("Do you want to auto fling " .. nearestPlayer.Name .. " [Y/N]")
                foundPlayers[nearestPlayer.UserId] = true
            elseif not hasTool and foundPlayers[nearestPlayer.UserId] then
                sendChatMessage(nearestPlayer.Name .. " has Activated Death Counter.")
                wait(1)
                sendChatMessage("Recommended use auto fling on" .. nearestPlayer.Name .. " !")
                foundPlayers[nearestPlayer.UserId] = false
            end
        end
    end
end

Checker_All.MouseButton1Click:Connect(function()
    checking = not checking
    if checking then
        foundPlayers = {}
        sendChatMessage("[Serious mode detection: nearest]")
        checkForTool()
    else
        sendChatMessage("[Serious mode detection: off]")
    end
end)

