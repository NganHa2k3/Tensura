local Checker_All = Instance.new("TextButton")
Checker_All.Parent = ScrollingFrame
Checker_All.BackgroundColor3 = Color3.new(0, 0, 0)
Checker_All.BackgroundTransparency = 0.5
Checker_All.Size = UDim2.new(0.03, 0, 0.9, 0)  -- Adjusted size for better visibility
