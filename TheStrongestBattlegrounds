}

local soundIds = {
    ["Teleport"] = "rbxassetid://405593386",
    ["Emote"] = "rbxassetid://12345678",
    ["Anim1"] = "rbxassetid://9044612350",
    ["Anim2"] = "rbxassetid://12345678",
