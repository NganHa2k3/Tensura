TpToolEDGE.TextColor3 = Color3.new(1, 1, 1)
TpToolEDGE.Font = Enum.Font.PatrickHand
TpToolEDGE.Text = "Dụng cụ dịch chuyển"
TpToolEDGE.Visible = true

TpToolEDGE.MouseButton1Click:Connect(function()
    local player = game.Players.LocalPlayer
local mouse = player:GetMouse()

-- Create the teleport tool
local TpTool = Instance.new("Tool")
TpTool.Name = "Teleport Tool"
TpTool.RequiresHandle = false
TpTool.Parent = player.Backpack

-- When the tool is activated
TpTool.Activated:Connect(function()
    local char = player.Character or workspace:FindFirstChild(player.Name)
    local hrp = char and char:FindFirstChild("HumanoidRootPart")
    if not char or not hrp then
        return warn("Failed to find HumanoidRootPart")
    end
    
    -- Get the position where the mouse is pointing
    local targetPosition = mouse.Hit.p
    
    -- Teleport the player to the clicked position, offsetting the Y by 3 units to avoid collision
    hrp.CFrame = CFrame.new(targetPosition.X, targetPosition.Y + 3, targetPosition.Z)
end)
end)

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")

local StareAtNearest = Instance.new("TextButton")
StareAtNearest.Parent = ScrollingFrame
StareAtNearest.BackgroundColor3 = Color3.new(0, 0, 0)
StareAtNearest.BackgroundTransparency = 0.5
StareAtNearest.Size = UDim2.new(0.03, 0, 0.9, 0)
StareAtNearest.TextScaled = true
StareAtNearest.Position = UDim2.new(0.48, 0, 0, 0)
StareAtNearest.TextColor3 = Color3.new(1, 1, 1)
StareAtNearest.Font = Enum.Font.PatrickHand
StareAtNearest.Text = "stare: off"

local isStaring = false
local stareLoop
local camera = game:GetService("Workspace").CurrentCamera
local targetPlayer = nil

local function getNearestPlayer()
    local nearestPlayer = nil
    local shortestDistance = math.huge

    for _, player in pairs(Players:GetPlayers()) do
        if player ~= Players.LocalPlayer and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
            local distance = (Players.LocalPlayer.Character.PrimaryPart.Position - player.Character.HumanoidRootPart.Position).magnitude
            if distance < shortestDistance then
                shortestDistance = distance
                nearestPlayer = player
            end
        end
    end
    return nearestPlayer
end

local function stareAtNearest()
    if not isStaring then
        if stareLoop then
            stareLoop:Disconnect()
            stareLoop = nil
        end
        camera.CameraSubject = Players.LocalPlayer.Character.Humanoid
        StareAtNearest.Text = "stare: off"
        targetPlayer = nil
        return
    end

    if not targetPlayer then
        targetPlayer = getNearestPlayer()
    end

    if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
        camera.CameraSubject = targetPlayer.Character.Humanoid

        local function stareFunc()
            if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") then
                -- Get the position of the target
                local targetPosition = targetPlayer.Character.HumanoidRootPart.Position
                local currentCameraPosition = camera.CFrame.Position
                
                -- Calculate direction from camera to target
                local direction = (targetPosition - currentCameraPosition).unit
                
                -- Maintain current position, but only change the orientation (rotation) of the camera
                camera.CFrame = CFrame.new(currentCameraPosition, currentCameraPosition + direction)
            end
        end

        stareLoop = RunService.RenderStepped:Connect(stareFunc)
        StareAtNearest.Text = "stare: on"
    end
end

StareAtNearest.MouseButton1Click:Connect(function()
    isStaring = not isStaring
    stareAtNearest()
end)

