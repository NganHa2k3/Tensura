-- Remove Existed One --
local player = game.Players.LocalPlayer
local screenGuiName = "TSBMyUIVN"

for _, gui in ipairs(player:WaitForChild("PlayerGui"):GetChildren()) do
    if gui:IsA("ScreenGui") and gui.Name == screenGuiName then
        gui:Destroy()
    end
end

-- Create UI elements
local ScreenGui = Instance.new("ScreenGui")
local ScrollingFrame = Instance.new("ScrollingFrame")
local ToggleButton = Instance.new("TextButton")

local ScrollingEmote = Instance.new("ScrollingFrame")
local ScrollingAnims = Instance.new("ScrollingFrame")
