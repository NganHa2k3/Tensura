local goalPosition4 = UDim2.new(-1, 0, 0.2, 0)  -- Original position

local tween3 = TweenService:Create(ScrollingJob, tweenInfo, {Position = goalPosition3})
local tween4 = TweenService:Create(ScrollingJob, tweenInfo, {Position = goalPosition4})

local isToggled1 = false
button1.MouseButton1Click:Connect(function()
    if isToggled1 then
        tween4:Play()
    else
        tween3:Play()
    end
    isToggled1 = not isToggled1
end)

